{
  "name": "mockapp",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "json-server": "^0.14.0",
    "mobx": "^5.0.4",
    "mobx-react": "^5.2.5",
    "node-sass-chokidar": "^1.3.3",
    "prop-types": "^15.6.2",
    "react": "^16.4.2",
    "react-dom": "^16.4.2",
    "react-router-dom": "^4.3.1",
    "react-scripts": "1.1.5",
    "react-transition-group": "^2.4.0"
  },
  "scripts": {
    "build-css": "node-sass-chokidar src/styles/scss/index.scss -o src/styles/css/",
    "watch-css": "npm run build-css && node-sass-chokidar src/styles/scss/ -o src/styles/css/ --watch --recursive",
    "start-js": "react-scripts start",
    "start": "npm-run-all -p watch-css start-js",
    "build-js": "react-scripts build",
    "build": "npm-run-all build-css build-js",
    "test:lint:js": "eslint --ext=js --ext=jsx src",
    "test:lint:scss": "stylelint \"**/*.scss\"",
    "test:unit": "react-scripts test --env=jsdom --coverage",
    "test": "run-s test:**",
    "eject": "react-scripts eject",
    "storybook": "start-storybook -p 9009 -s public",
    "build-storybook": "build-storybook -s public"
  },
  "devDependencies": {
    "eslint": "^5.4.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.1",
    "eslint-plugin-react": "^7.11.1",
    "npm-run-all": "^4.1.3",
    "stylelint": "^9.5.0",
    "stylelint-config-standard": "^18.2.0",
    "stylelint-order": "^1.0.0",
    "stylelint-scss": "^3.3.0",
    "@storybook/react": "^3.4.10",
    "@storybook/addon-actions": "^3.4.10",
    "@storybook/addon-links": "^3.4.10",
    "@storybook/addons": "^3.4.10",
    "babel-core": "^6.26.3",
    "babel-runtime": "^6.26.0"
  }
}
